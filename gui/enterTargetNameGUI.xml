<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<GUI onOpen="onOpen" onClose="onClose" onCreate="onCreate">
    <GuiElement profile="newLayer"/>
    <Bitmap profile="dialogFullscreenBg" id="dialogBg"/>
    <GuiElement profile="fs25_dialogBg" id="dialogElement" height="280px">
        <ThreePartBitmap profile="fs25_dialogBgMiddleSmall"/>
        <ThreePartBitmap profile="fs25_dialogBgTopSmall"/>
        <ThreePartBitmap profile="fs25_dialogBgBottom"/>
        <GuiElement profile="fs25_dialogContentContainer">
            <Text profile="fs25_dialogTitle" id="titleElement" position="0px -40px"/>
            <ThreePartBitmap profile="fs25_lineSeparatorTop" absoluteSizeOffset="100px 0px" position="0px -80px"/>
            <Text profile="fs25_dialogText" id="dialogTextElement" text="$l10n_gui_ad_enterTargetNameText" position="0px -110px"/>
            <TextInput profile="fs25_dialogTextInput" id="textInputElement" onEnterPressed="onEnterPressed" onEscPressed="onEscPressed" enterWhenClickOutside="false" focusInit="onOpen" maxCharacters="30">
                <ThreePartBitmap profile="fs25_textInputBg"/>
                <Bitmap profile="fs25_dialogTextInputIconBox">
                    <Bitmap profile="fs25_dialogTextInputIcon"/>
                </Bitmap>
            </TextInput>
        </GuiElement>
        <BoxLayout profile="fs25_dialogButtonBox" id="buttonsCreateElement">
            <Button profile="buttonOK" text="$l10n_button_ok" onClick="onClickOk"/>
            <Bitmap profile="fs25_dialogButtonBoxSeparator"/>
            <Button profile="buttonBack" text="$l10n_button_back" onClick="onClickBack"/>
        </BoxLayout>
        <BoxLayout profile="fs25_dialogButtonBox" id="buttonsEditElement">
            <Button profile="buttonActivate" text="$l10n_button_delete" onClick="onClickActivate" />
            <Bitmap profile="fs25_dialogButtonBoxSeparator"/>
            <Button profile="buttonCancel" text="$l10n_button_reset" onClick="onClickCancel" />
            <Bitmap profile="fs25_dialogButtonBoxSeparator"/>
            <Button profile="buttonOK" text="$l10n_button_rename" onClick="onClickOk" />
            <Bitmap profile="fs25_dialogButtonBoxSeparator"/>
            <Button profile="buttonBack" text="$l10n_button_back" onClick="onClickBack" />
        </BoxLayout>
    </GuiElement>
</GUI>
